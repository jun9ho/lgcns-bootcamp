<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lgcns.inspire_spring.test.dao.TestMapper">
    <select id = "selectRow"
            parameterType = "com.lgcns.inspire_spring.test.domain.TestRequestDTO"
            resultType = "com.lgcns.inspire_spring.test.domain.TestResponseDTO">
        SELECT emp_id, emp_name, email, salary
        FROM EMPLOYEE
        WHERE EMP_ID= #{emp_id} AND EMAIL = #{email}
    </select>
</mapper>